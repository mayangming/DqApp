<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:state_selected="false"
        android:drawable="@mipmap/checkbox_selected" /><!--设置选中图片-->
    <item android:state_checked="false" android:state_selected="false"
        android:drawable="@mipmap/checkbox_normal" /><!--设置未选中图片-->
    <item android:state_selected="true"
        android:drawable="@mipmap/checkbox_pressed"/>
</selector>
